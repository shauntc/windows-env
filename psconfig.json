{
    "name": "shauntc",

    "historyLength": 10000,
    "usePrompt": true,
    "prompt": {
        "git": true,
        "time": true,
        "admin": true
    },
    "keyBindings": {
        "UpArrow": "HistorySearchBackward",
        "DownArrow": "HistorySearchForward",
        "Tab": "MenuComplete"
    },
    "functionAlias": {
        "home": "cd $env:USERPROFILE"
    },

    "printCommands": true,
    "printConfig": false,

    "bashCommands": true,
    "vsCommands": true,
    "convenienceCommands": true
}